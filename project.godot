#ou720,dependendodasuaescolhawindow/stretch/mode="2d"

[layer_names]

2d_physics/layer_1="Tile"
2d_physics/layer_2="SlideTile"
2d_physics/layer_3="Player"
2d_physics/layer_4="Enemy"

[physics]

