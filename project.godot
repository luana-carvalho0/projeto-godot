; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "ParallaxBackground",
"class": "Background",
"language": "GDScript",
"path": "res://scripts/env/background.gd"
}, {
"base": "Area2D",
"class": "DetectionArea",
"language": "GDScript",
"path": "res://scripts/enemy/detection_area.gd"
}, {
"base": "KinematicBody2D",
"class": "EnemyTemplate",
"language": "GDScript",
"path": "res://scripts/enemy/enemy_template.gd"
}, {
"base": "Sprite",
"class": "EnemyTexture",
"language": "GDScript",
"path": "res://scripts/enemy/enemy_texture.gd"
}, {
"base": "EnemyTexture",
"class": "WhaleTexture",
"language": "GDScript",
"path": "res://scripts/enemy/whale/texture.gd"
} ]

_global_script_class_icons={
"Background": "",
"DetectionArea": "",
"EnemyTemplate": "",
"EnemyTexture": "",
"WhaleTexture": ""
}

[application]

config/name="projeto-godot"
config/icon="res://icon.png"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=640  # ou 1280, dependendo da sua escolha
window/size/test_height=360  # ou 720, dependendo da sua escolha
window/stretch/mode="2d"
window/stretch/aspect="keep"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
